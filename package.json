    "@smui/top-app-bar": "^7.0.0-beta.3",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.5.0",
    "smui-theme": "^7.0.0-beta.3",
    "svelte": "^3.54.0",
